{
  "name": "overlook-security-sandbox",
  "version": "1.0.0",
  "description": "Security testing sandbox for Overlook collaborative platform",
  "main": "sandbox-server.js",
  "scripts": {
    "start": "node sandbox-server.js",
    "dev": "nodemon sandbox-server.js",
    "test": "jest",
    "docker:build": "docker build -t overlook-security-sandbox .",
    "docker:run": "docker run -p 3002:3002 --env-file .env overlook-security-sandbox",
    "docker:compose": "docker-compose up -d"
  },
  "keywords": [
    "security",
    "penetration-testing",
    "vulnerability-assessment",
    "sandbox",
    "exploit-testing"
  ],
  "author": "Overlook Security Team",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "uuid": "^9.0.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/node": "^20.8.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/overlook/security-sandbox"
  },
  "bugs": {
    "url": "https://github.com/overlook/security-sandbox/issues"
  },
  "homepage": "https://github.com/overlook/security-sandbox#readme"
